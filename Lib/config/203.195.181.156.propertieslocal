drivers=com.mysql.jdbc.Driver

# 数据库连接池配置

# 每300秒检查所有连接池中的空闲连接 
idleConnectionTestPeriod=300

# 设置一个未使用的连接的存活时间
IdleMaxAgeInMinutes=300

# 设置分区的个数
partitionCount=4

# 每个分区最大连接数
maxConnectionsPerPartition=10

# 每个分区最大连接数
minConnectionsPerPartition=1

# 连接的增长数量
AcquireIncrement=5

# 释放线程数
releaseHelperThreads=4

# 是否开启关闭监视 
CloseConnectionWatch=false

# 超时未关闭监控，60秒
CloseConnectionWatchTimeout=60000

# 连接超时，60秒
ConnectionTimeout=60000

# 是否进行数据类型的验证，debug时开启(1)，正式环境关闭(0)
DataTypeCheck=1

# 是否开启DBWatch
DBWatch=0

# 是否进行事务安全检查
TransactionCheck=0

